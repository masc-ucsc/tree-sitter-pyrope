
Room <- (model:string)
Car  <- (model:string)

volvo:Car = (model="V40")

xx:Room = (model="2 bedroom")


either <- fun(arg:Room) { // Structural type check (Room equivalent o Car)
  puts("you have model {}", arg.model)
}

either(volvo)
either(xx)

only_car <- fun(arg is Car) {
  puts("you have a nice car model {}", arg.model)
}

only_car(volvo) // OK
only_car(xx)    // compile error

